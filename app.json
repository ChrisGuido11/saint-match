{
  "expo": {
    "name": "Saint Match",
    "slug": "saint-match",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "saintmatch",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F1EA"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.saintmatch.app",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSUserNotificationsUsageDescription": "Saint Match sends daily reminders and novena prayer notifications to support your spiritual journey."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F5F1EA"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.saintmatch.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#8B9D83"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ed4582e0-a8d0-4ebc-bd19-508113005f69"
      }
    },
    "owner": "crisguido",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ed4582e0-a8d0-4ebc-bd19-508113005f69"
    }
  }
}
